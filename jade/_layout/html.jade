mixin css(csss)
    each css in csss
        | <link rel="stylesheet" href="/css/#{css}.css">
mixin js(jss)
    each js in jss
        script(src="/js/#{js}.js")

doctype html
html
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
    link(rel="stylesheet" href="/css/default.css")
    script(src="/js/libs/jquery.js" charset="utf-8")
  body
    block middle
    block js